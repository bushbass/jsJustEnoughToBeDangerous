romStorage","generated":{"line":54,"column":0},"source":"script.js","original":{"line":63,"column":2}},{"name":"createCardsFromStorage","generated":{"line":54,"column":2},"source":"script.js","original":{"line":63,"column":2}},{"generated":{"line":54,"column":24},"source":"script.js","original":{"line":63,"column":24}},{"generated":{"line":55,"column":0},"source":"script.js","original":{"line":64,"column":1}},{"generated":{"line":55,"column":1},"source":"script.js","original":{"line":60,"column":0}}],"sources":{"script.js":"import '/style.scss';\nconsole.log(\n  'initial local storage',\n  window.localStorage.getItem('creatureArray')\n);\nlet creatureList = [];\nconst formName = document.querySelector('#name');\nconst formBreed = document.querySelector('#breed');\nconst testDiv = document.querySelector('#testdiv');\nconst card = document.querySelector('#card');\nconst deleteButton = document.querySelector('#delete-button');\nconst formDescription = document.querySelector('#description');\nconst button = document.querySelector('button');\nlet creatureHTML = '';\nlet cards = '';\n\nif (window.localStorage.getItem('creatureArray')) {\n  creatureList = JSON.parse(window.localStorage.getItem('creatureArray'));\n  createListFromStorage();\n  testDiv ? (testDiv.innerHTML = creatureHTML) : null;\n}\n\nfunction createListFromStorage() {\n  creatureList.forEach(\n    creature => (creatureHTML += `<li>${creature.name}</li>`)\n  );\n}\nbutton\n  ? button.addEventListener('click', e => {\n      e.preventDefault();\n      creatureList.unshift({\n        name: formName.value,\n        breed: formBreed.value,\n        description: formDescription.value\n      });\n      creatureHTML += `<li>${creatureList[0].name}</li>`;\n      testDiv ? (testDiv.innerHTML = creatureHTML) : null;\n\n      window.localStorage.setItem(\n        'creatureArray',\n        JSON.stringify(creatureList)\n      );\n    })\n  : null;\n\nconsole.log('creature list', creatureList);\nfunction createCardsFromStorage() {\n  creatureList.forEach(creature => {\n    cards += `<div class=\"card\">\n        <h4>${creature.name}</h4>\n        <p>${creature.breed}</p>\n        <p>${creature.description}</p>\n      </div>`;\n    console.log(cards);\n  });\n}\ncreateCardsFromStorage();\ncard ? (card.innerHTML = cards) : null;\n\ndeleteButton.addEventListener('click', () => {\n  window.localStorage.setItem('creatureArray', []);\n  createListFromStorage();\n  createCardsFromStorage();\n});\n"},"lineCount":null}},"hash":"1527f1743f9d53cbb4551d28a92a7a5b","cacheData":{"env":{}}}